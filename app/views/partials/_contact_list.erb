<div class="col-md-3">
  <div class="panel panel-default">
    <div class="panel-heading"><h3>My Contacts</h3></div>
    <div class="panel-body">
      <% @contacts.each do |contact| %>
        Name: <%= contact.name %> <br>
        Phone: <%= contact.cellphone %> <br>
        <hr>
      <% end %>
    </div>
  </div>
</div>
