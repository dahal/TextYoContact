<div class="col-md-3">
  <div class="panel panel-default">
    <div class="panel-heading"><h3>My Contacts</h3></div>
    <div class="panel-body contacts">
      <% @contacts.each do |contact| %>
      <div class="contact<%= contact.id %>">
        <span class="name<%= contact.id %>"><%= contact.name %></span> <br>
        <span class="phone<%= contact.id %>"><%= contact.cellphone %><br>
        <button class="textButton fa fa-comments fa-lg"></button>
        <button class="editButton fa fa-pencil-square fa-lg"></button>
        <button class="deleteButton fa fa-trash fa-lg"></button>
        <hr>
      </div>
      <% end %>
    </div>
  </div>
</div>
