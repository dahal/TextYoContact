<!DOCTYPE html>
<html>
<head>
  <title>TextYoContact</title>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag 'http://maxcdn.bootstrapcdn.com/bootswatch/3.2.0/flatly/bootstrap.min.css' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'http://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js' %>
  <%= javascript_include_tag 'http://cdnjs.cloudflare.com/ajax/libs/handlebars.js/2.0.0/handlebars.min.js' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render "partials/navbar" %>
  <div class="container header">
    <%= yield %>
  </div>
  <%= render "partials/contact_error" %>
  <%= render "partials/new_contact" %>
</body>
</html>
